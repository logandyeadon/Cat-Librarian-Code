using System.Collections;
using System.Collections.Generic;
using TMPro;
using UnityEngine;

public class ShopInterface : MonoBehaviour
{
    GameObject character;
    GameObject doormat;
    GameObject objectHit;
    [SerializeField] GameObject bookshelfShopNumberOnesObject;
    [SerializeField] GameObject bookshelfShopNumberTensObject;
    [SerializeField] GameObject tableShopNumberOnesObject;
    [SerializeField] GameObject tableShopNumberTensObject;
    [SerializeField] GameObject chairShopNumberOnesObject;
    [SerializeField] GameObject chairShopNumberTensObject;
    [SerializeField] GameObject deskShopNumberOnesObject;
    [SerializeField] GameObject deskShopNumberTensObject;
    [SerializeField] GameObject computerShopNumberOnesObject;
    [SerializeField] GameObject computerShopNumberTensObject;

    [SerializeField] GameObject bookshelfShopPriceOnesObject;
    [SerializeField] GameObject bookshelfShopPriceTensObject;
    [SerializeField] GameObject bookshelfShopPriceHundredsObject;
    [SerializeField] GameObject tableShopPriceOnesObject;
    [SerializeField] GameObject tableShopPriceTensObject;
    [SerializeField] GameObject tableShopPriceHundredsObject;
    [SerializeField] GameObject chairShopPriceOnesObject;
    [SerializeField] GameObject chairShopPriceTensObject;
    [SerializeField] GameObject chairShopPriceHundredsObject;
    [SerializeField] GameObject deskShopPriceOnesObject;
    [SerializeField] GameObject deskShopPriceTensObject;
    [SerializeField] GameObject deskShopPriceHundredsObject;
    [SerializeField] GameObject computerShopPriceOnesObject;
    [SerializeField] GameObject computerShopPriceTensObject;
    [SerializeField] GameObject computerShopPriceHundredsObject;

    RaycastHit2D hit;

    [SerializeField] CharacterMovement characterMovement;
    [SerializeField] SceneManagerScript sceneManager;
    [SerializeField] ShopCustomizationScript shopCustomizationScript;
    [SerializeField] SpriteController spriteController;

    int bookshelfShopNumber = 1;
    int bookshelfShopNumberOnes = 1;
    int bookshelfShopNumberTens;
    int tableShopNumber = 1;
    int tableShopNumberOnes = 1;
    int tableShopNumberTens;
    int chairShopNumber = 1;
    int chairShopNumberOnes = 1;
    int chairShopNumberTens;
    int deskShopNumber = 1;
    int deskShopNumberOnes = 1;
    int deskShopNumberTens;
    int computerShopNumber = 1;
    int computerShopNumberOnes = 1;
    int computerShopNumberTens;

    int bookshelfShopPrice;
    int bookshelfShopPriceOnes;
    int bookshelfShopPriceTens;
    int bookshelfShopPriceHundreds;
    int tableShopPrice;
    int tableShopPriceOnes;
    int tableShopPriceTens;
    int tableShopPriceHundreds;
    int chairShopPrice;
    int chairShopPriceOnes;
    int chairShopPriceTens;
    int chairShopPriceHundreds;
    int deskShopPrice;
    int deskShopPriceOnes;
    int deskShopPriceTens;
    int deskShopPriceHundreds;
    int computerShopPrice;
    int computerShopPriceOnes;
    int computerShopPriceTens;
    int computerShopPriceHundreds;

    void Start()
    {
        character = GameObject.Find("Character");
        doormat = GameObject.Find("Doormat");
        SetShopPrices();
    }

    void Update()
    {
        if(Input.GetMouseButtonDown(0))
        {
            RaycastHit2D hit = Physics2D.Raycast(Camera.main.ScreenToWorldPoint(Input.mousePosition), Vector2.zero);

            if(hit.collider != null)
            {
                objectHit = hit.collider.gameObject;
                BuyItemFromShop();
            }
        }
    }

    void BuyItemFromShop()
    {
        if(objectHit.name == "Close Shop Bookmark")
        {
            character.GetComponent<Collider2D>().enabled = true;
            characterMovement.cantMove = false;
            character.transform.position = doormat.transform.position;
            character.GetComponent<SpriteRenderer>().enabled = true;
            characterMovement.beenShopping = true;
            sceneManager.shopSign.transform.position = new Vector2(sceneManager.shopSign.transform.position.x, sceneManager.shopSign.transform.position.y - 5);
            sceneManager.currentFurniture = null;
            ResetShopNumbers();
            SetShopPrices();
        }

        if(objectHit.name == "First Buy Button" && sceneManager.goldAmount >= 5)
        {
            sceneManager.goldAmount -= bookshelfShopNumber * 10;
            sceneManager.bookshelfNumber += bookshelfShopNumber;
        }
        else if(objectHit.name == "First Sell Button" && sceneManager.bookshelfNumber >= bookshelfShopNumber)
        {
            sceneManager.goldAmount += bookshelfShopNumber * 10;
            sceneManager.bookshelfNumber -= bookshelfShopNumber;
        }
        else if(objectHit.name == "First Increase Arrow")
        {
            if(bookshelfShopNumberOnes < 9)
            {
                bookshelfShopNumber++;
                bookshelfShopNumberOnes++;
            }
            else if(bookshelfShopNumberOnes == 9 && bookshelfShopNumberTens < 9)
            {
                bookshelfShopNumber++;
                bookshelfShopNumberTens++;
                bookshelfShopNumberOnes = 0;
            }
            bookshelfShopPrice = bookshelfShopNumber * 10;
            bookshelfShopPriceOnes = bookshelfShopPrice % 10;
            bookshelfShopPriceTens = (bookshelfShopPrice % 100) / 10;
            bookshelfShopPriceHundreds = (bookshelfShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }
        else if(objectHit.name == "First Decrease Arrow")
        {
            if(bookshelfShopNumberOnes >= 1 && bookshelfShopNumber > 1)
            {
                bookshelfShopNumber--;
                bookshelfShopNumberOnes--;
            }
            else if(bookshelfShopNumberOnes == 0)
            {
                bookshelfShopNumber--;
                bookshelfShopNumberTens --;
                bookshelfShopNumberOnes = 9;
            }
            bookshelfShopPrice = bookshelfShopNumber * 10;
            bookshelfShopPriceOnes = bookshelfShopPrice % 10;
            bookshelfShopPriceTens = (bookshelfShopPrice % 100) / 10;
            bookshelfShopPriceHundreds = (bookshelfShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }

        if(objectHit.name == "Second Buy Button" && sceneManager.goldAmount >= 5)
        {
            sceneManager.goldAmount -= tableShopNumber * 5;
            sceneManager.tableNumber += tableShopNumber;
        }
        else if(objectHit.name == "Second Sell Button" && sceneManager.tableNumber >= tableShopNumber)
        {
            sceneManager.goldAmount += tableShopNumber * 5;
            sceneManager.tableNumber -= tableShopNumber;
        }
        else if(objectHit.name == "Second Increase Arrow")
        {
            if(tableShopNumberOnes < 9)
            {
                tableShopNumber++;
                tableShopNumberOnes++;
            }
            else if(tableShopNumberOnes == 9 && tableShopNumberTens < 9)
            {
                tableShopNumber++;
                tableShopNumberTens++;
                tableShopNumberOnes = 0;
            }
            tableShopPrice = tableShopNumber * 5;
            tableShopPriceOnes = tableShopPrice % 10;
            tableShopPriceTens = (tableShopPrice % 100) / 10;
            tableShopPriceHundreds = (tableShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }
        else if(objectHit.name == "Second Decrease Arrow")
        {
            if(tableShopNumberOnes >= 1 && tableShopNumber > 1)
            {
                tableShopNumber--;
                tableShopNumberOnes--;
            }
            else if(tableShopNumberOnes == 0)
            {
                tableShopNumber--;
                tableShopNumberTens --;
                tableShopNumberOnes = 9;
            }
            tableShopPrice = tableShopNumber * 5;
            tableShopPriceOnes = tableShopPrice % 10;
            tableShopPriceTens = (tableShopPrice % 100) / 10;
            tableShopPriceHundreds = (tableShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }

        if(objectHit.name == "Third Buy Button" && sceneManager.goldAmount >= 5)
        {
            sceneManager.goldAmount -= chairShopNumber * 5;
            sceneManager.chairNumber += chairShopNumber;
        }
        else if(objectHit.name == "Third Sell Button" && sceneManager.chairNumber >= chairShopNumber)
        {
            sceneManager.goldAmount += chairShopNumber * 5;
            sceneManager.chairNumber -= chairShopNumber;
        }
        else if(objectHit.name == "Third Increase Arrow")
        {
            if(chairShopNumberOnes < 9)
            {
                chairShopNumber++;
                chairShopNumberOnes++;
            }
            else if(chairShopNumberOnes == 9 && chairShopNumberTens < 9)
            {
                chairShopNumber++;
                chairShopNumberTens++;
                chairShopNumberOnes = 0;
            }
            chairShopPrice = chairShopNumber * 5;
            chairShopPriceOnes = chairShopPrice % 10;
            chairShopPriceTens = (chairShopPrice % 100) / 10;
            chairShopPriceHundreds = (chairShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }
        else if(objectHit.name == "Third Decrease Arrow")
        {
            if(chairShopNumberOnes >= 1 && chairShopNumber > 1)
            {
                chairShopNumber--;
                chairShopNumberOnes--;
            }
            else if(chairShopNumberOnes == 0)
            {
                chairShopNumber--;
                chairShopNumberTens --;
                chairShopNumberOnes = 9;
            }
            chairShopPrice = chairShopNumber * 5;
            chairShopPriceOnes = chairShopPrice % 10;
            chairShopPriceTens = (chairShopPrice % 100) / 10;
            chairShopPriceHundreds = (chairShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }

        if(objectHit.name == "Fourth Buy Button" && sceneManager.goldAmount >= 5)
        {
            sceneManager.goldAmount -= deskShopNumber * 5;
            sceneManager.deskNumber += deskShopNumber;
        }
        else if(objectHit.name == "Fourth Sell Button" && sceneManager.deskNumber >= deskShopNumber)
        {
            sceneManager.goldAmount += deskShopNumber;
            sceneManager.deskNumber -= deskShopNumber;
        }
        else if(objectHit.name == "Fourth Increase Arrow")
        {
            if(deskShopNumberOnes < 9)
            {
                deskShopNumber++;
                deskShopNumberOnes++;
            }
            else if(deskShopNumberOnes == 9 && deskShopNumberTens < 9)
            {
                deskShopNumber++;
                deskShopNumberTens++;
                deskShopNumberOnes = 0;
            }
            deskShopPrice = deskShopNumber * 5;
            deskShopPriceOnes = deskShopPrice % 10;
            deskShopPriceTens = (deskShopPrice % 100) / 10;
            deskShopPriceHundreds = (deskShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }
        else if(objectHit.name == "Fourth Decrease Arrow")
        {
            if(deskShopNumberOnes >= 1 && deskShopNumber > 1)
            {
                deskShopNumber--;
                deskShopNumberOnes--;
            }
            else if(deskShopNumberOnes == 0)
            {
                deskShopNumber--;
                deskShopNumberTens --;
                deskShopNumberOnes = 9;
            }
            deskShopPrice = deskShopNumber * 5;
            deskShopPriceOnes = deskShopPrice % 10;
            deskShopPriceTens = (deskShopPrice % 100) / 10;
            deskShopPriceHundreds = (deskShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }

        if(objectHit.name == "Fifth Buy Button" && sceneManager.goldAmount >= 5)
        {
            sceneManager.goldAmount -= computerShopNumber * 10;
            sceneManager.computerNumber += computerShopNumber;
        }
        else if(objectHit.name == "Fifth Sell Button" && sceneManager.computerNumber >= computerShopNumber)
        {
            sceneManager.goldAmount += computerShopNumber;
            sceneManager.computerNumber -= computerShopNumber;
        }
        else if(objectHit.name == "Fifth Increase Arrow")
        {
            if(computerShopNumberOnes < 9)
            {
                computerShopNumber++;
                computerShopNumberOnes++;
            }
            else if(computerShopNumberOnes == 9 && computerShopNumberTens < 9)
            {
                computerShopNumber++;
                computerShopNumberTens++;
                computerShopNumberOnes = 0;
            }
            computerShopPrice = computerShopNumber * 10;
            computerShopPriceOnes = computerShopPrice % 10;
            computerShopPriceTens = (computerShopPrice % 100) / 10;
            computerShopPriceHundreds = (computerShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }
        else if(objectHit.name == "Fifth Decrease Arrow")
        {
            if(computerShopNumberOnes >= 1 && computerShopNumber > 1)
            {
                computerShopNumber--;
                computerShopNumberOnes--;
            }
            else if(computerShopNumberOnes == 0)
            {
                computerShopNumber--;
                computerShopNumberTens --;
                computerShopNumberOnes = 9;
            }
            computerShopPrice = computerShopNumber * 10;
            computerShopPriceOnes = computerShopPrice % 10;
            computerShopPriceTens = (computerShopPrice % 100) / 10;
            computerShopPriceHundreds = (computerShopPrice % 1000) / 100;
            CheckShopNumberSprite();
        }

        if(objectHit.name == "Customize Shop Sticky")
        {
            shopCustomizationScript.spawnedItemBoxHolder = false;
            sceneManager.customizingShop = true;
            characterMovement.beenShopping = true;
            characterMovement.cantMove = false;
            shopCustomizationScript.CustomizeShopInterface();
            character.transform.position = GameObject.Find("Scene Manager").transform.position;
            ResetShopNumbers();
            SetShopPrices();
        }
    }

    void SetShopPrices()
    {
        bookshelfShopPrice = bookshelfShopNumber * 10;
        bookshelfShopPriceOnes = bookshelfShopPrice % 10;
        bookshelfShopPriceTens = (bookshelfShopPrice % 100) / 10;
        bookshelfShopPriceHundreds = (bookshelfShopPrice % 1000) / 100;

        tableShopPrice = tableShopNumber * 5;
        tableShopPriceOnes = tableShopPrice % 10;
        tableShopPriceTens = (tableShopPrice % 100) / 10;
        tableShopPriceHundreds = (tableShopPrice % 1000) / 100;
                    
        chairShopPrice = chairShopNumber * 5;
        chairShopPriceOnes = chairShopPrice % 10;
        chairShopPriceTens = (chairShopPrice % 100) / 10;
        chairShopPriceHundreds = (chairShopPrice % 1000) / 100;

        deskShopPrice = deskShopNumber * 5;
        deskShopPriceOnes = deskShopPrice % 10;
        deskShopPriceTens = (deskShopPrice % 100) / 10;
        deskShopPriceHundreds = (deskShopPrice % 1000) / 100;

        computerShopPrice = computerShopNumber * 10;
        computerShopPriceOnes = computerShopPrice % 10;
        computerShopPriceTens = (computerShopPrice % 100) / 10;
        computerShopPriceHundreds = (computerShopPrice % 1000) / 100;

        CheckShopNumberSprite();
    }

    void ResetShopNumbers()
    {
        bookshelfShopNumber = 1;
        bookshelfShopNumberOnes = 1;
        bookshelfShopNumberTens = 0;
        tableShopNumber = 1;
        tableShopNumberOnes = 1;
        tableShopNumberTens = 0;
        chairShopNumber = 1;
        chairShopNumberOnes = 1;
        chairShopNumberTens = 0;
        deskShopNumber = 1;
        deskShopNumberOnes = 1;
        deskShopNumberTens = 0;
        computerShopNumber = 1;
        computerShopNumberOnes = 1;
        computerShopNumberTens = 0;
        CheckShopNumberSprite();
    }

    void CheckShopNumberSprite()
    {
        if(bookshelfShopNumberOnes == 0)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberZero;
        }
        else if(bookshelfShopNumberOnes == 1)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberOne;
        }
        else if(bookshelfShopNumberOnes == 2)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberTwo;
        }
        else if(bookshelfShopNumberOnes == 3)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberThree;
        }
        else if(bookshelfShopNumberOnes == 4)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFour;
        }
        else if(bookshelfShopNumberOnes == 5)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFive;
        }
        else if(bookshelfShopNumberOnes == 6)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(bookshelfShopNumberOnes == 7)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
        }
        else if(bookshelfShopNumberOnes == 8)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberEight;
        }
        else if(bookshelfShopNumberOnes == 9)
        {
            bookshelfShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberNine;
        }

        if(bookshelfShopNumberTens == 0)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberZero;
        }
        else if(bookshelfShopNumberTens == 1)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberOne;
        }
        else if(bookshelfShopNumberTens == 2)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberTwo;
        }
        else if(bookshelfShopNumberTens == 3)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberThree;
        }
        else if(bookshelfShopNumberTens == 4)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFour;
        }
        else if(bookshelfShopNumberTens == 5)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFive;
        }
        else if(bookshelfShopNumberTens == 6)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(bookshelfShopNumberTens == 7)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
        }
        else if(bookshelfShopNumberTens == 8)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberEight;
        }
        else if(bookshelfShopNumberTens == 9)
        {
            bookshelfShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberNine;
        }

        if(tableShopNumberOnes == 0)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberZero;
        }
        else if(tableShopNumberOnes == 1)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberOne;
        }
        else if(tableShopNumberOnes == 2)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberTwo;
        }
        else if(tableShopNumberOnes == 3)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberThree;
        }
        else if(tableShopNumberOnes == 4)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFour;
        }
        else if(tableShopNumberOnes == 5)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFive;
        }
        else if(tableShopNumberOnes == 6)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(tableShopNumberOnes == 7)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
        }
        else if(tableShopNumberOnes == 8)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberEight;
        }
        else if(tableShopNumberOnes == 9)
        {
            tableShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberNine;
        }

        if(tableShopNumberTens == 0)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberZero;
        }
        else if(tableShopNumberTens == 1)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberOne;
        }
        else if(tableShopNumberTens == 2)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberTwo;
        }
        else if(tableShopNumberTens == 3)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberThree;
        }
        else if(tableShopNumberTens == 4)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFour;
        }
        else if(tableShopNumberTens == 5)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFive;
        }
        else if(tableShopNumberTens == 6)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(tableShopNumberTens == 7)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
        }
        else if(tableShopNumberTens == 8)
        {
            tableShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberEight;
        }
        else if(tableShopNumberTens == 9)
        {
