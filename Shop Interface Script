            chairShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(chairShopNumberTens == 7)
        {
            chairShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
        }
        else if(chairShopNumberTens == 8)
        {
            chairShopNumberTensObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberEight;
