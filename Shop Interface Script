            deskShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberFive;
        }
        else if(deskShopNumberOnes == 6)
        {
            deskShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSix;
        }
        else if(deskShopNumberOnes == 7)
        {
            deskShopNumberOnesObject.GetComponent<SpriteRenderer>().sprite = spriteController.shopNumberSeven;
